        "\n",
        "#입력\n",
        "for i in range(9):\n",
        "  n=int(input())\n",
        "  if n<100:\n",
        "    list.append(n)\n",
        "\n",
        "#계산\n",
        "max=list[0] # list[0]가 제일 크면\n",
        "find=0\n",
        "for n in list:\n",
        "  if max<n:\n",
        "    max=n\n",
        "    find=list.index(max)+1\n",
        "     \n",
        "#출력\n",
        "print(int(max))\n",
        "print(int(find))\n",
        "    "
      ]
    }
  ]
